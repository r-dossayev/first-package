{
    "name": "r-dossayev/first-package",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "first",
        "package"
    ],
    "homepage": "https://github.com/r-dossayev/first-package",
    "license": "MIT",
    "authors": [
        {
            "name": "r-dossayev",
            "homepage": "https://github.com/r-dossayev",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "dossayev\\FPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dossayev\\FPackage\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
